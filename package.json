{
    "name": "serverless-esbuild-template",
    "devDependencies": {
        "@types/aws-lambda": "^8.10.59",
        "@types/jest": "^26.0.9",
        "@types/node": "^14.0.27",
        "@types/node-fetch": "^2.5.7",
        "@types/serverless": "^1.72.5",
        "@types/tcp-port-used": "^1.0.0",
        "@typescript-eslint/eslint-plugin": "^3.8.0",
        "@typescript-eslint/parser": "^3.8.0",
        "dotenv": "^8.2.0",
        "esbuild": "^0.6.18",
        "esbuild-register": "^1.0.2",
        "eslint": "^7.6.0",
        "eslint-config-prettier": "^6.11.0",
        "eslint-plugin-import": "^2.22.0",
        "eslint-plugin-jest": "^23.20.0",
        "eslint-plugin-promise": "^4.2.1",
        "esm": "^3.2.25",
        "execa": "^4.0.3",
        "jest": "^26.2.2",
        "node-fetch": "^2.6.0",
        "npm-run-all": "^4.1.5",
        "pm2": "^4.4.0",
        "prettier": "^2.0.5",
        "serverless": "^1.78.1",
        "serverless-esbuild": "^1.3.0",
        "serverless-offline": "^8.0.0",
        "tcp-port-used": "^1.0.1",
        "ts-jest": "^26.1.4",
        "ts-node": "^8.10.2",
        "typescript": "^3.9.7"
    },
    "scripts": {
        "test": "jest",
        "pm2": "pm2",
        "ts": "node -r esm -r esbuild-register -r dotenv/config",
        "eslint": "eslint --fix '**/*.ts' '**/*.js'",
        "prettier": "prettier '**/*.ts' '**/*.js' --write",
        "lint": "npm-run-all eslint prettier",
        "offline:dev": "yarn ts scripts/offline.ts dev",
        "offline:start": "yarn ts scripts/offline.ts start",
        "offline:stop": "yarn ts scripts/offline.ts stop",
        "manual-build": "esbuild src/handler.ts --bundle --outdir=.build",
        "deploy": "sls deploy",
        "destroy": "sls remove"
    },
    "dependencies": {
        "aws-sdk": "^2.932.0",
        "ulid": "^2.3.0"
    }
}
